{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@config/*": [
                "src/config/*"
            ],
            "@modules/*": [
                "src/modules/*"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ]
        }
    }
}